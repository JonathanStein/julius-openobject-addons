<?xml version="1.0" ?>
<openerp>
	<data>

		<record id="account_coda_import_view_extended" model="ir.ui.view">
			<field name="name">Import Coda Statement</field>
			<field name="model">account.coda.import</field>
			<field name="inherit_id" ref="l10n_be_coda.account_coda_import_view" />
			<field name="type">form</field>
			<field name="arch" type="xml">
				<field name="coda_data" position="before">
					<group colspan="4" col="4">
						<field name="journal_id"/>
						<field name="def_payable"/>
						<field name="def_receivable"/>
						<field name="awaiting_account"/>
						<newline/>
						<group colspan="4" col="6">
							<field name="filter_id"/>
							<field name="date_format"/>
							<field name="statement_update"/>
						</group>
					</group>
				</field>
			</field>
		</record>

	</data>
</openerp>
