<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data noupdate="1"> 
	
		<!-- Automatic Mail Template Creation -->
		<!-- Manager email -->
		
		<record id="failed_survey_manager" model="email.template">
            <field name="name">Failed survey - Manager Email</field>
            <field name="model_id" ref="model_hr_certificate"/>
            <field name="email_from">noreply@localhost</field>
            <field name="email_to">noreply@localhost</field>
            <field name="subject">Failed survey - Manager Email</field>
        </record>
        
        <!-- User email -->
		<record id="failed_survey_user" model="email.template">
            <field name="name">Failed survey - User Email</field>
            <field name="model_id" ref="model_hr_certificate"/>
            <field name="email_from">noreply@localhost</field>
            <field name="email_to">noreply@localhost</field>
            <field name="subject">Failed survey - User Email</field>
        </record>
        
	</data>
</openerp>
